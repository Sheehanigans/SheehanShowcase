@model CarDealership.UI.Models.SpecialListVM

@{
    ViewBag.Title = "Specials";
}

<h2>Specials</h2>

<div class="row">
    <div class="col-xs-12">
        @foreach(var special in Model.Specials)
        {
             <div class="col-xs-12 specialsListBoarder">
                 <p><strong>@special.SpecialTitle</strong></p>
                 <p>@special.SpecialMessage</p>
             </div>
        }
    </div>
</div>